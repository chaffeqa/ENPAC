<%= javascript_include_tag "modernizr-1.6.min.js" %>

<link rel="shortcut icon" type="image/png" href='/<%= subdomain %>/template/favicon.png' />
<link rel="apple-touch-icon" href="/apple-touch-icon.png">

<%= stylesheet_link_tag("reset", "addons/colorbox", "event_calendar",
                        "/#{subdomain}/stylesheets/contents",
                        "/#{subdomain}/stylesheets/template",
                        "phusion", :cache => "#{subdomain}-css") %>

<%= csrf_meta_tag %>

